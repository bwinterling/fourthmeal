<h1>Log in</h1>

<%= form_tag sessions_path, id:"login-form" do %>
  <p>
    <%= label_tag :email %><br />
    <%= text_field_tag :email, params[:email] %>
  </p>
  <p>
    <%= label_tag :password %><br />
    <%= password_field_tag :password %>
  </p>
  <div class="button"><%= submit_tag "Log In" %></div>
<% end %>